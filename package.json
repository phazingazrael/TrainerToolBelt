{
  "name": "Trainer-ToolBelt",
  "version": "0.5.0",
  "description": "An on-screen display of team members for use with OBS and Pokemon streaming.",
  "license": "MIT",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js > log-file.txt",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev:server": "cd client && yarn build && cd .. && yarn start",
    "start": "node server.js"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "level": "^6.0.1",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0",
    "nodemon": "^2.0.7",
    "oakdex-pokedex": "^0.4.5",
    "subleveldown": "^5.0.1"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/esausilva/example-create-react-app-express.git"
  },
  "bugs": {
    "url": "https://github.com/esausilva/example-create-react-app-express/issues"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app"
  ]
}
